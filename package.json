{
  "name": "AsitaAja",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "yarn static-code-analysis && yarn dynamic-code-analysis",
    "static-code-analysis": "yarn tslint && yarn typecheck && yarn check-format",
    "dynamic-code-analysis": "jest --coverage",
    "tslint": "tslint -p \".\" -t stylish",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "check-format": "prettier --check \"src/**/*.{ts,tsx}\"",
    "testWatch": "jest --coverage --watch"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "dayjs": "^1.8.18",
    "hoist-non-react-statics": "^3.3.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "react": "16.9.0",
    "react-dom": "^16.9.0",
    "react-native": "0.61.2",
    "react-native-fast-image": "^7.0.2",
    "react-native-gesture-handler": "^1.5.0",
    "react-native-modal": "^11.5.3",
    "react-native-reanimated": "^1.4.0",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-native-simple-auth": "^2.4.0",
    "react-native-tab-view": "^2.11.0",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^1.10.3",
    "react-navigation-tabs": "^2.5.6",
    "react-navigation-transitions": "^1.0.12",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "ts-optchain": "^0.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "@babel/runtime": "^7.6.3",
    "@react-native-community/async-storage": "^1.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.5",
    "@types/react-native": "^0.60.19",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-navigation": "^3.0.8",
    "@types/react-redux": "^7.1.5",
    "@types/react-test-renderer": "^16.9.0",
    "@types/redux-logger": "^3.0.7",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "^16.10.2",
    "redux-devtools-extension": "^2.13.8",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.1.0",
    "tslint-react-recommended": "^1.0.15",
    "typescript": "^3.6.3"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/src/.*\\.(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "rnpm": {
    "assets": [
      "fonts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test -u"
    }
  }
}
